{
    "version": 2,
    "builds": [{
      "src": "**/*.html",
      "use": "@now/static"
    }],
    "routes": [{
      "src": "/",
      "dest": "/index.html"
    }, {
      "src": "/about",
      "dest": "/about.html"
    }, {
      "src": "/article",
      "dest": "/article.html"
    }, {
      "src": "/contact",
      "dest": "/contact.html"
    }]
  }
  